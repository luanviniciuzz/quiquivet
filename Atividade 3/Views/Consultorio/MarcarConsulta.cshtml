@model Consulta
@{
    ViewData["Title"] = "Registro";
}
<body style="background-color: rgb(133, 184, 181)">
<h2>Registrar Cliente</h2>
<p id="pMensagem">@ViewBag.Mensagem</p>
<div class="container">
    <div class="col-sm-8">
    <form asp-action="MarcarConsulta" method="POST">
    <div class="form-group">
     <label asp-for="clienteConsulta" for="inputNameConsulta" class="col-sm-2 col-form-label">Nome do Cliente:</label>
    <div class="col-sm-10">
     <input asp-for="clienteConsulta" class="form-control" id="inputNameConsulta">
    </div>
    </div>
    <div class="form-group">
     <label asp-for="detalhesConsulta" for="inputObs" class="col-sm-2 col-form-label">Observações:</label>
    <div class="col-sm-10">
     <input asp-for="detalhesConsulta" class="form-control" id="inputObs">
    </div>
    </div>    
    <div class="form-group">
     <label asp-for="medicoConsulta" for="inputMed" class="col-sm-2 col-form-label">Médico Veterinário:</label>
    <div class="col-sm-10">
     <input asp-for="medicoConsulta" class="form-control" id="inputMed">
    </div>
    </div>    
    <div class="form-group">
     <label asp-for="precoConsulta" for="inputPreco" class="col-sm-2 col-form-label">Valor:</label>
    <div class="col-sm-10">
     <input asp-for="precoConsulta" class="form-control" id="inputPreco">
    </div>
    </div>
    <div class="form-group">
     <label asp-for="dtConsulta" for="inputDataConsulta" class="col-sm-2 col-form-label">Data:</label>
    <div class="col-sm-10">
     <input asp-for="dtConsulta" class="form-control" id="inputDataConsulta">
    </div>
    </div>
    <p>
        <input type="submit" value="Marcar" />
    </p>
</form>
    </div>
    <a class="text-body" href="/Home/ConsultaClinica" style="text-decoration:none">Voltar</a>
</div>
<br>
</body>